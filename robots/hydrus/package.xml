<?xml version="1.0"?>
<package>
  <name>hydrus</name>
  <version>1.0.0</version>
  <description>The basic packages for hydrus</description>

  <maintainer email="chou@jsk.imi.i.u-tokyo.ac.jp">Bakui Chou</maintainer>
  <license>BSD</license>
  <url type="website">https://github.com/tongtybj/aerial_robot</url>
  <author email="chou@jsk.imi.i.u-tokyo.ac.jp">Bakui Chou</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>aerial_robot_msgs</build_depend>
  <build_depend>aerial_robot_base</build_depend>
  <build_depend>aerial_robot_model</build_depend>
  <build_depend>spinal</build_depend>
  <build_depend>std_srvs</build_depend>
  <build_depend>dynamixel_controllers</build_depend>
  <build_depend>eigen_conversions</build_depend>
  <build_depend>tf_conversions</build_depend>
  <build_depend>cmake_modules</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>orocos_kdl</build_depend>
  <build_depend>kdl_parser</build_depend>
  <build_depend>pluginlib</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>aerial_robot_msgs</run_depend>
  <run_depend>aerial_robot_base</run_depend>
  <run_depend>aerial_robot_model</run_depend>
  <run_depend>aerial_robot_simulation</run_depend>
  <run_depend>spinal</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>dynamixel_controllers</run_depend>
  <run_depend>eigen_conversions</run_depend>
  <run_depend>tf_conversions</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>message_runtime</run_depend>
  <run_depend>kdl_parser</run_depend>
  <run_depend>orocos_kdl</run_depend>
  <run_depend>pluginlib</run_depend>

  <export>
    <hydrus plugin="${prefix}/differential_kinematics_cost.xml" />
    <hydrus plugin="${prefix}/differential_kinematics_constraint.xml" />
  </export>

</package>
